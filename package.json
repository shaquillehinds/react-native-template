{
  "name": "reactnativetemplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "clean": "rm -rf node_modules/ && cd ios && rm -rf Pods && cd ..",
    "clean-android": "rm -rf node_modules/ && yarn && cd android && ./gradlew clean && cd ../",
    "clean-all": "yarn clean && yarn setup && cd android && ./gradlew clean && cd ../",
    "setup": "yarn && npx pod-install",
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator='iPhone 14 Pro Max'",
    "ios2": "yarn ios && react-native run-ios --simulator='iPhone 14'",
    "ios3": "yarn ios2 && react-native run-ios --simulator='iPhone 14 Pro'",
    "ios-device": "react-native run-ios --device",
    "cache-reset": "yarn cache clean",
    "reinit-pod": "cd ios && rm -rf Pods && pod install && cd ..",
    "release-android": "cd android && ./gradlew bundleRelease && cd ..",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.10",
    "@react-navigation/bottom-tabs": "^6.4.0",
    "@react-navigation/native": "^6.0.13",
    "@react-navigation/native-stack": "^6.9.0",
    "@react-navigation/stack": "^6.3.1",
    "axios": "^0.27.2",
    "events": "^3.3.0",
    "expo": "^46.0.0",
    "expo-font": "^10.2.0",
    "expo-splash-screen": "^0.16.2",
    "lottie-ios": "3.4.0",
    "lottie-react-native": "^5.1.4",
    "react": "18.1.0",
    "react-native": "0.70.1",
    "react-native-dotenv": "^3.3.1",
    "react-native-gesture-handler": "^2.6.1",
    "react-native-modal": "^13.0.1",
    "react-native-reanimated": "^2.10.0",
    "react-native-safe-area-context": "^4.3.4",
    "react-native-screens": "^3.17.0",
    "react-native-svg": "^13.2.0",
    "react-native-vector-icons": "^9.2.0",
    "react-redux": "^8.0.2",
    "redux": "^4.2.0",
    "redux-thunk": "^2.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/events": "^3.0.0",
    "@types/jest": "^26.0.23",
    "@types/react-native": "^0.70.0",
    "@types/react-native-vector-icons": "^6.4.12",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.72.1",
    "react-test-renderer": "18.1.0",
    "typescript": "^4.8.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/$1",
      "^test/(.*)$": "<rootDir>/../test/$1",
      "^@utils/(.*)$": "<rootDir>/utils/$1",
      "^@hooks/(.*)$": "<rootDir>/hooks/$1",
      "^@store/(.*)$": "<rootDir>/store/$1",
      "^@styles/(.*)$": "<rootDir>/styles/$1",
      "^@transports/(.*)$": "<rootDir>/transports/$1",
      "^@components/(.*)$": "<rootDir>/components/$1",
      "^@navigators/(.*)$": "<rootDir>/navigators/$1",
      "^@routers/(.*)$": "<rootDir>/routers/$1",
      "^@assets/(.*)$": "<rootDir>/../assets/$1",
      "^@screens/(.*)$": "<rootDir>/screens$1",
      "^@configuration": "<rootDir>/../configuration.ts"
    },
    "rootDir": "src"
  }
}
