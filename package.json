{
  "name": "reactnativetemplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "version:bump:major": "node scripts/versionBump/versionBump.js major",
    "version:bump:minor": "node scripts/versionBump/versionBump.js minor",
    "version:bump:patch": "node scripts/versionBump/versionBump.js patch",
    "version:bump:bundle": "node scripts/versionBump/versionBump.js bundle",
    "clean": "rm -rf node_modules/ && yarn clean-pods",
    "clean-deep": "rm yarn.lock && rm -rf node_modules/ && yarn clean-pods-deep",
    "clean-pods": "cd ios && rm -rf Pods && cd ..",
    "clean-pods-deep": "cd ios && rm -rf Pods && rm Podfile.lock && cd ..",
    "clean-android": "rm -rf node_modules/ && yarn && cd android && ./gradlew clean && cd ../",
    "clean-all": "yarn clean && yarn setup && cd android && ./gradlew clean && cd ../",
    "setup": "yarn && npx pod-install",
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator='iPhone 15 Pro Max'",
    "ios2": "yarn ios && react-native run-ios --simulator='iPhone 15'",
    "ios3": "yarn ios2 && react-native run-ios --simulator='iPhone 14 Pro'",
    "ios-device": "react-native run-ios --device",
    "cache-reset": "yarn cache clean",
    "reinit-pod": "cd ios && rm -rf Pods && pod install && cd ..",
    "release-android": "cd android && ./gradlew bundleRelease && cd ..",
    "test": "jest",
    "lint": "eslint ."
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "4",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/blur": "4.4.0",
    "@react-native-community/clipboard": "1.5.1",
    "@react-native-community/datetimepicker": "^8.0.0",
    "@react-native-community/netinfo": "11.3.1",
    "@react-native-firebase/app": "19.2.2",
    "@react-native-firebase/auth": "19.2.2",
    "@react-native-firebase/messaging": "19.2.2",
    "@react-native-masked-view/masked-view": "0.3.1",
    "@react-navigation/bottom-tabs": "6.5.20",
    "@react-navigation/material-top-tabs": "6.6.13",
    "@react-navigation/native": "6.1.17",
    "@react-navigation/native-stack": "6.9.26",
    "@react-navigation/stack": "6.3.29",
    "@shaquillehinds/graphql-query-builder": "^1.0.3",
    "@shaquillehinds/react-native-draggable-flatlist": "0.0.8",
    "axios": "1.6.8",
    "color": "4.2.3",
    "events": "3.3.0",
    "expo": "^51.0.0",
    "expo-constants": "16.0.1",
    "expo-font": "12.0.4",
    "expo-image-picker": "~15.0.4",
    "expo-splash-screen": "0.27.4",
    "formik": "2.4.6",
    "lottie-ios": "4.4.1",
    "lottie-react-native": "6.7.2",
    "react": "18.2.0",
    "react-native": "0.74.1",
    "react-native-compressor": "^1.8.24",
    "react-native-device-info": "^10.13.2",
    "react-native-dotenv": "3.4.11",
    "react-native-dropdown-picker": "5.4.6",
    "react-native-gesture-handler": "2.16.2",
    "react-native-keyboard-aware-scroll-view": "0.9.5",
    "react-native-linear-gradient": "2.8.3",
    "react-native-maps": "^1.15.1",
    "react-native-modal": "13.0.1",
    "react-native-pager-view": "6.3.1",
    "react-native-reanimated": "3.11.0",
    "react-native-redash": "18.1.3",
    "react-native-safe-area-context": "4.10.1",
    "react-native-screens": "3.31.1",
    "react-native-shared-element": "0.8.9",
    "react-native-svg": "15.2.0",
    "react-native-tab-view": "3.5.2",
    "react-native-vector-icons": "10.1.0",
    "react-native-webview": "^13.10.0",
    "react-navigation-shared-element": "3.1.3",
    "react-redux": "9.1.2",
    "redux": "5.0.1",
    "redux-thunk": "3.1.0",
    "yup": "1.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-proposal-export-namespace-from": "7.18.9",
    "@babel/plugin-transform-private-methods": "^7.24.1",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.74.83",
    "@react-native/eslint-config": "0.74.83",
    "@react-native/metro-config": "0.74.83",
    "@react-native/typescript-config": "0.74.83",
    "@types/color": "3.0.6",
    "@types/events": "3.0.3",
    "@types/react": "^18.2.6",
    "@types/react-native-vector-icons": "6.4.18",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "babel-plugin-module-resolver": "5.0.2",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/$1",
      "^test/(.*)$": "<rootDir>/../test/$1",
      "^@utils/(.*)$": "<rootDir>/utils/$1",
      "^@hooks/(.*)$": "<rootDir>/hooks/$1",
      "^@store/(.*)$": "<rootDir>/store/$1",
      "^@styles/(.*)$": "<rootDir>/styles/$1",
      "^@transports/(.*)$": "<rootDir>/transports/$1",
      "^@components/(.*)$": "<rootDir>/components/$1",
      "^@navigators/(.*)$": "<rootDir>/navigators/$1",
      "^@routers/(.*)$": "<rootDir>/routers/$1",
      "^@assets/(.*)$": "<rootDir>/../assets/$1",
      "^@screens/(.*)$": "<rootDir>/screens$1",
      "^@configuration": "<rootDir>/../configuration.ts"
    },
    "rootDir": "src"
  },
  "engines": {
    "node": ">=18"
  }
}
